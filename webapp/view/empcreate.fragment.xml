<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
>
    <f:SimpleForm
        editable="true"
        layout="ResponsiveGridLayout"
        labelSpanL="2"
        labelSpanM="2"
        emptySpanL="7"
        emptySpanM="7"
        columnsL="3"
        columnsM="3"
    >
        <f:content>
            <Label text="Employee id" />
            <Input id= "Idempid1"  />
            <Label text="Employee Name" />
            <Input id = "Idempname" />
            <Label text="Disignation" />
            <Input id = "Iddesignation"/>
            <Label text="Salary" />
            <Input id = "Idsalary"/>
            <Label text="Date of Joing" />
            <DatePicker id = "Iddojcre" ></DatePicker>
        </f:content>
    </f:SimpleForm>
</core:FragmentDefinition>
